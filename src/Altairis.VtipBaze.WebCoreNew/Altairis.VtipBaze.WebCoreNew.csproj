    <PackageReference Include="DotVVM.HotReload.AspNetCore" Version="4.2.0-preview04-5554570822" />
    <PackageReference Include="System.ServiceModel.Syndication" Version="7.0.0" />
  </ItemGroup>
  <ItemGroup>
    <None Include="compilerconfig.json" />
    <None Include="wwwroot\Scripts\jquery-2.0.3.intellisense.js" />
    <None Include="wwwroot\Scripts\_references.js" />
    <None Include="wwwroot\Styles\main.scss" />
  </ItemGroup>
  <ItemGroup>
